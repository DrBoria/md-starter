{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Native",
  "extends": "./react.json",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "lib": ["es2019"],
    "moduleResolution": "node",
    "noEmit": true,
    "target": "esnext",
    "skipLibCheck": true
  }
} 
