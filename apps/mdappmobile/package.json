{
  "name": "mdappmobile",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "react-native start",
    "dev": "react-native run-android",
    "clean": "cd ./android && ./gradlew clean && cd ..",
    "pods-install": "arch -x86_64 pod install --repo-update",
    "dev-ios": "react-native run-ios --simulator='iPhone 16 pro'",
    "dark-mode": "adb shell \"cmd uimode night yes\"",
    "light-mode": "adb shell \"cmd uimode night no\"",
    "network": "adb -s emulator-5554 reverse tcp:3000 tcp:3000",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@apollo/react-hooks": "^4.0.0",
    "@keystone-6/core": "6.3.0",
    "@md/api": "workspace:*",
    "@md/native": "workspace:*",
    "@md/styles": "workspace:*",
    "@md/utils": "workspace:*",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-native/codegen": "^0.76.0",
    "@react-native/gradle-plugin": "^0.76.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "react": "18.3.1",
    "react-native": "0.76.0",
    "react-native-mmkv": "^3.1.0",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "^3.35.0",
    "react-native-svg": "^15.8.0",
    "react-router-native": "^6.27.0",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@md/eslint": "workspace:*",
    "@md/types": "workspace:*",
    "@react-native-community/cli": "15.0.0-alpha.2",
    "@react-native-community/cli-platform-android": "15.0.0-alpha.2",
    "@react-native-community/cli-platform-ios": "15.0.0-alpha.2",
    "@react-native/babel-preset": "0.76.0",
    "@react-native/eslint-config": "0.76.0",
    "@react-native/metro-config": "0.76.0",
    "@react-native/typescript-config": "0.76.0",
    "@types/react": "^18.3.11",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-jest": "^29.6.3",
    "eslint": "^9.9.1",
    "eslint-plugin-import": "^2.30.0",
    "jest": "^29.6.3",
    "prettier": "3.3.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.3.1",
    "reactotron-react-native": "^5.1.9",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  }
}
