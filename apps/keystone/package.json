{
  "name": "keystone",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node clearKeystoneDev.js",
    "db:push": "keystone prisma db push",
    "db:reset": "keystone prisma db push --force-reset",
    "db:makemigration": "ci/check-db-migrations --fix",
    "start": "keystone start",
    "ui-kit": "styleguidist server",
    "build": "keystone build",
    "db:migrate": "keystone prisma migrate deploy",
    "prod": "npm run db:push && npm run start",
    "generate:roles": "node ./populateRoles.js",
    "format": "prettier --check",
    "format:fix": "prettier --write",
    "lint": "eslint --report-unused-disable-directives-severity=off",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --esModuleInterop --skipLibCheck --noEmit --jsx react",
    "ci:postinstall": "pnpm prisma generate && pnpm keystone build --no-ui --no-server",
    "ci:check": "pnpm ci:check:code && pnpm ci:check:db",
    "ci:check:code": "pnpm typecheck && pnpm format:fix . && pnpm lint",
    "ci:check:db": "bash ci/check-db-migrations"
  },
  "dependencies": {
    "@apollo/client": "^3.11.5",
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/client-transfer": "^3.637.0",
    "@aws-sdk/lib-storage": "^3.637.0",
    "@aws-sdk/rds-signer": "^3.716.0",
    "@hubspot/api-client": "^11.2.0",
    "@keystone-6/auth": "8.0.0",
    "@keystone-6/core": "6.3.0",
    "@keystone-6/fields-document": "9.0.0",
    "@keystone-ui/button": "^7.0.2",
    "@keystone-ui/core": "^5.0.2",
    "@keystone-ui/fields": "^7.2.0",
    "@keystone-ui/icons": "^6.0.2",
    "@keystone-ui/modals": "^6.0.3",
    "@keystone-ui/notice": "^6.0.2",
    "@keystone-ui/toast": "^6.0.2",
    "@md/api": "workspace:*",
    "@md/components": "workspace:*",
    "@md/sections": "workspace:*",
    "@md/styles": "workspace:*",
    "@md/utils": "workspace:*",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "5.19.0",
    "@redis/client": "^1.6.0",
    "@sendgrid/client": "^8.1.3",
    "@types/multer": "^1.4.12",
    "aws-sdk": "^2.1692.0",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-graphql": "^0.12.0",
    "fast-deep-equal": "^3.1.3",
    "google-auth-library": "^9.14.2",
    "graphql": "^16.9.0",
    "http-status-codes": "^2.3.0",
    "jose": "^5.8.0",
    "moment": "^2.30.1",
    "multer": "1.4.5-lts.1",
    "next": "^14.2.10",
    "pg": "^8.13.1",
    "pkg-dir": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-timezone-select": "^3.2.8",
    "request-promise-native": "^1.0.9",
    "styled-components": "^6.1.13",
    "typescript": "^5.5.4",
    "url": "^0.11.4",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@svgr/webpack": "^8.1.0",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/request-promise-native": "^1.0.21",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.38.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.9.1",
    "eslint-plugin-import": "^2.30.0",
    "globals": "^15.9.0",
    "is-react-prop": "^1.0.0",
    "lint-staged": "^15.2.10",
    "msw": "^2.4.1",
    "postcss": "^8.4.44",
    "postcss-easy-import": "^4.0.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "prisma": "5.19.0",
    "react-styleguidist": "^13.1.3",
    "style-loader": "^4.0.0",
    "typescript-eslint-parser": "^22.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.94.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pnpm typecheck",
      "pnpm lint:fix",
      "pnpm format:fix"
    ]
  },
  "msw": {
    "workerDirectory": [
      "admin/utils/ui-kit/msw/public"
    ]
  }
}
